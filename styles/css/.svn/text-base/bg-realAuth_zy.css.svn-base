@charset "utf-8";
.bold {
  font-weight: bold;
}
.el-header {
  border-radius: 0px;
  margin-bottom: 0px;
}
.el-header .container .navbar-header a {
  color: #ffffff;
}
.el-header .container .navbar-header a i {
  font-size: 18px;
}
.el-header .container .navbar-header a span {
  font-size: 24px;
  font-style: italic;
}
.el-header .container .navbar-header p {
  font-size: 20px;
  color: #eeeeee;
  margin-bottom: 0px;
  height: 50px;
  line-height: 50px;
}
.el-header .container .navbar-header p span {
  color: #10adff;
}
.el-header .container .collapse .navbar-nav li a {
  color: #ffffff;
}
.el-header .container .collapse .navbar-nav li a:hover {
  color: #10adff;
  text-decoration: underline;
}
.el-nav {
  margin-bottom: 0px;
}
.el-nav .container .navbar-header .navbar-brand {
  display: inline-block;
  padding-top: 22px;
  height: 80px;
}
.el-nav .container .navbar-header .navbar-brand > img {
  width: 80px;
}
.el-nav .container .navbar-collapse .navbar-nav li {
  min-width: 80px;
}
.el-nav .container .navbar-collapse .navbar-nav li a {
  height: 80px;
  line-height: 80px;
  padding-bottom: 0px;
  padding-top: 0px;
  font-size: 18px;
  text-align: center;
}
.el-nav .container .navbar-collapse .navbar-nav li a:hover {
  color: #0a91ff;
  background-color: #e8e8e8;
}
.el-main {
  margin-top: 10px;
}
.el-main .container {
  padding: 0px;
}
.el-main .container .panel-heading {
  line-height: 34px;
}
.el-main .container .panel-body {
  padding: 0px;
}
.el-main .container .panel-body .table {
  margin-bottom: 0px;
}
.el-main .container .table th {
  font-size: 16px;
}
.el-main .container .table td {
  height: 40px ;
  line-height: 40px;
}
.el-footer {
  padding-top: 30px;
  padding-right: 0px;
  padding-left: 0px;
  background-color: #f8f8f8;
}
.el-footer .row .col-sm-8 {
  padding-right: 0px;
}
.el-footer .row .col-sm-8 p {
  height: 20px;
  line-height: 20px;
  margin-bottom: 25px;
}
.el-footer .row .col-sm-2 {
  padding-right: 0px;
}
.el-footer .row .col-sm-2 p {
  font-size: 16px;
  text-align: center;
}
.el-footer .row img {
  width: 140px;
  height: 70px;
}
.el-footer .row #weixin {
  width: 150px;
  height: 150px;
}
